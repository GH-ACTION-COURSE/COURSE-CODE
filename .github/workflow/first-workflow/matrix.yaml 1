name: Matrix
on: [push]

jobs: 
   node-Matrix:
     runs-on: ubuntu-latest
     strategy:
        Matrix:
           node-version: [14,15,16]
     steps: 
       - run: node -v
       - uses: actions/setup-node@v3
         with:
           node-version: ${{ Matrix.node-version }}
       - run: node -v3
