name: Matrix
on: [push]

jobs: 
   node-Matrix:
     strategy:
        Matrix:
           os: [ubuntu-latest, mac-latest]
           node-version: [14,15,16]
     runs-on: ${{ Matrix.os }}
     steps: 
       - run: node -v
       - uses: actions/setup-node@v3
         with:
           node-version: ${{ Matrix.node-version }}
       - run: node -v3
